
generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}


//model: dev leaderboard:
model Developer {
  id           String      @id @default(cuid())
  username     String      @unique
  score        Int        
  createdAt    DateTime     @default(now())
  
  @@index([score(sort: Desc)])
}

